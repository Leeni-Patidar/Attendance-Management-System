{
  "name": "attendance-management-system",
  "version": "1.0.0",
  "description": "Complete Attendance Management System with React Frontend and Node.js Backend",
  "private": true,
  "scripts": {
    "install:backend": "cd AMS-Backend && npm install",
    "install:frontend": "cd AMS-Frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd AMS-Backend && npm run dev",
    "dev:frontend": "cd AMS-Frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "setup": "cd AMS-Backend && npm run setup",
    "reset": "cd AMS-Backend && npm run reset",
    "build:frontend": "cd AMS-Frontend && npm run build",
    "build:backend": "cd AMS-Backend && npm run start",
    "start:backend": "cd AMS-Backend && npm start",
    "start:frontend": "cd AMS-Frontend && npm run preview",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "test:backend": "cd AMS-Backend && npm test",
    "lint:frontend": "cd AMS-Frontend && npm run lint",
    "health": "curl -s http://localhost:5000/health && echo '\nBackend is running!' || echo 'Backend is not running!'"
  },
  "keywords": [
    "attendance",
    "management",
    "qr-code",
    "react",
    "nodejs",
    "mongodb",
    "jwt",
    "education"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repo-url"
  }
}